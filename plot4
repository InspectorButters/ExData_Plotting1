getwd()library("data.table")full <- fread(input = "household_power_consumption.txt", stringsAsFactors = FALSE, na.strings = "?")#Convert "Date" column from character to Datefull <- full[, Date:= as.Date(x = Date, format = "%d/%m/%Y")]#Only the relevant datespart <- full[ Date >= as.Date("2007-02-01") & Date <= as.Date("2007-02-02"),]#Setup a 2 x 2 'grid'par(mfrow = c(2,2))#upper-left plotwith(part, plot(x = as.POSIXct(paste(Date, Time),                               format ="%Y-%m-%d %H:%M:%S"),                y = Global_active_power,                type = "l",                xlab = "",                ylab = "Global Active Power(kilowatts)"))#upper-right plotwith(part, plot(x = as.POSIXct(paste(Date, Time),                               format ="%Y-%m-%d %H:%M:%S"),                y = Voltage,                type = "l",                xlab = "datetime",                ylab = "Voltage"))#lower-left plotwith(part, plot(x = as.POSIXct(paste(Date, Time),                               format ="%Y-%m-%d %H:%M:%S"),                y = Sub_metering_1,                type = "l",                xlab = "",                ylab = "Energy sub metering)"))with(part, lines(x = as.POSIXct(paste(Date, Time),                                format ="%Y-%m-%d %H:%M:%S"),                 y = Sub_metering_2, col = "red"))with(part, lines(x = as.POSIXct(paste(Date, Time),                                format ="%Y-%m-%d %H:%M:%S"),                 y = Sub_metering_3, col = "blue"))legend("topright", lwd = 1, cex = 1, col = c("black", "red", "blue"),       legend = c("Sub_metering_1", "Sub_metering_2", "Sub_metering_3"))#lower-right plotwith(part, plot(x = as.POSIXct(paste(Date, Time),                               format ="%Y-%m-%d %H:%M:%S"),                y = Global_reactive_power,                type = "l",                xlab = "datetime",                ylab = "Global_reactive_power"))
