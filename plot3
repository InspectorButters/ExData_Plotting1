getwd()library("data.table")full <- fread(input = "household_power_consumption.txt", stringsAsFactors = FALSE, na.strings = "?")#Convert "Date" column from character to Datefull <- full[, Date:= as.Date(x = Date, format = "%d/%m/%Y")]#Only the relevant datespart <- full[ Date >= as.Date("2007-02-01") & Date <= as.Date("2007-02-02"),]#Plot submetering 1 line in blackwith(part, plot(x = as.POSIXct(paste(Date, Time),                               format ="%Y-%m-%d %H:%M:%S"),                y = Sub_metering_1,                type = "l",                xlab = "",                ylab = "Energy sub metering)"))#Plot submetering 2 line in redwith(part, lines(x = as.POSIXct(paste(Date, Time),                               format ="%Y-%m-%d %H:%M:%S"),                y = Sub_metering_2, col = "red"))                #Plot submetering 3 line in bluewith(part, lines(x = as.POSIXct(paste(Date, Time),                                format ="%Y-%m-%d %H:%M:%S"),                 y = Sub_metering_3, col = "blue"))                legend("topright", lwd = 2, cex = 1, col = c("black", "red", "blue"),       legend = c("Sub_metering_1", "Sub_metering_2", "Sub_metering_3"))                
